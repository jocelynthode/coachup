<%= form_for :googlemap, url: googlemaps_path do |f| %>
    <p>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </p>

    <p>
      <%= f.label :latitude %><br>
      <%= f.text_field :latitude %>
    </p>

    <p>
      <%= f.label :longitude %><br>
      <%= f.text_field :longitude %>
    </p>

    <p>
      <%= f.label :description %><br>
      <%= f.text_field :description %>
    </p>

    <p>
      <%= f.label :address %><br>
      <%= f.text_field :address %>
    </p>
    <p>
      <%= f.submit %>
    </p>

<% end %>

<%= link_to 'Back', googlemaps_path %>